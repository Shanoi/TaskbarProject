public class Run extends Lecture{

    BufferedReader file = new BufferedReader(new FileReadder(path));

    @Override
    public void execute()
    {
	Syntaxe line = new Syntaxe();
	ComputationalModel.init();
	
	while( (line = file.readline()) != null )
	    {
		if(line.isShort())
		    shortSyntaxe.run();
		else
		    longSyntaxe.run();
		
	    }


    }




}
